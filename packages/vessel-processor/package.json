{
  "name": "vessel-processor",
  "version": "1.0.0",
  "description": "Kafka consumer for processing vessel state updates and generating notifications",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:reset": "tsx src/index.ts --reset",
    "dev:from-beginning": "tsx src/index.ts --from-beginning",
    "info": "tsx src/index.ts --info",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:reset": "node dist/index.js --reset",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "aws-msk-iam-sasl-signer-js": "^1.0.0",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "kafkajs-snappy": "^1.1.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}
