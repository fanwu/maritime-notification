# Production environment variables for AWS deployment
# Copy this to .env and fill in your values

# Mapbox token (required - get one at https://mapbox.com)
NEXT_PUBLIC_MAPBOX_TOKEN=your-mapbox-token-here

# App URLs - replace with your EC2 public IP or domain
NEXT_PUBLIC_APP_URL=http://YOUR_EC2_IP:3000
NEXT_PUBLIC_WS_URL=ws://YOUR_EC2_IP:3000

# CORS (adjust for production)
ALLOWED_ORIGIN=*
